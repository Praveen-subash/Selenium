
public class Selenium4_Arrays_ArrayLists {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Arrays
		int[] arr1 = new int[3];
		arr1[1] = 1;
		arr1[2] = 2;
		arr1[2] = 3;
		
		int[] arr2 = {4,5,6};
		
		for(int i=0;i<=arr1.length;i++) {
			
			System.out.println(arr1[i]);
		}
		
		for(int i:arr2) {
			
			System.out.println(i);
		}
		

	}

}
