
public class Dummy {

	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Accessing the static method geek() and  
        // field by class name itself. 
      
		int i=0;
		int j=i++ + ++i;
		
		System.out.println(j);
		System.out.println(i);
		
		
		
		
		
	}

}
